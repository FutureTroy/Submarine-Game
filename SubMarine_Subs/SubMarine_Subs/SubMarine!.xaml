        <Rectangle Fill="#FFF3FFFE" HorizontalAlignment="Left" Height="39" Margin="539,136,0,0" StrokeThickness="3" VerticalAlignment="Top" Width="107" OpacityMask="#FFCFECEA"/>
        <Rectangle Fill="#FFCFECEA" HorizontalAlignment="Left" Height="26" Margin="549,180,0,0" StrokeThickness="3" VerticalAlignment="Top" Width="107" OpacityMask="#FFCFECEA"/>
